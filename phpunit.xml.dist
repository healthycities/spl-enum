<?xml version="1.0" encoding="UTF-8"?>
<phpunit backupGlobals="false"
         colors="true"
         verbose="true"
         bootstrap="bootstrap.php">

    <php>
        <ini name="error_reporting" value="-1" />
    </php>

    <testsuites>
        <testsuite name="SplEnum Test Suite">
            <directory suffix=".php">./tests/</directory>
        </testsuite>
    </testsuites>

</phpunit>
